{"ast":null,"code":"export default {\n  name: \"WelCome\",\n  data() {\n    return {\n      categories: [],\n      showLoginOptions: false,\n      selectedRole: null,\n      carouselIndex: 0,\n      productIndices: {},\n      images: [\"biscuits.jpeg\", \"sliderr2.png\", \"grain lowest.jpeg\", \"vegetable -2.jpg\"]\n    };\n  },\n  methods: {\n    getProductIndex(categoryId) {\n      return this.productIndices[categoryId] || 0;\n    },\n    nextProduct(categoryId) {\n      const category = this.categories.find(cat => cat.id === categoryId);\n      if (!category) return;\n      const maxIndex = Math.ceil(category.products.length - 1);\n      this.productIndices[categoryId] = Math.min(this.getProductIndex(categoryId) + 1, maxIndex);\n    },\n    prevProduct(categoryId) {\n      this.productIndices[categoryId] = Math.max(this.getProductIndex(categoryId) - 1, 0);\n    },\n    nextImage() {\n      this.carouselIndex = (this.carouselIndex + 1) % this.images.length;\n    },\n    prevImage() {\n      this.carouselIndex = (this.carouselIndex - 1 + this.images.length) % this.images.length;\n    }\n  }\n};","map":{"version":3,"names":["name","data","categories","showLoginOptions","selectedRole","carouselIndex","productIndices","images","methods","getProductIndex","categoryId","nextProduct","category","find","cat","id","maxIndex","Math","ceil","products","length","min","prevProduct","max","nextImage","prevImage"],"sources":["C:\\Users\\mahak\\Downloads\\21f2000845-freshcart\\Mad 2 project\\freshcart\\src\\components\\welcome.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <!-- Navbar -->\n    <nav class=\"navbar\">\n      <h1 class=\"app-title\">FreshCart App</h1>\n      <button class=\"login-button\" @click=\"showLoginOptions = true\">\n        Login / Sign Up\n      </button>\n    </nav>\n\n    <!-- Carousel Section -->\n    <div class=\"carousel-container\">\n      <div\n        class=\"carousel\"\n        :style=\"{ transform: `translateX(-${carouselIndex * 100}%)` }\"\n      >\n        <img\n          v-for=\"(image, index) in images\"\n          :key=\"index\"\n          :src=\"require(`@/photo/${image}`)\"\n          alt=\"carousel image\"\n          class=\"carousel-image\"\n        />\n      </div>\n      <button class=\"carousel-button prev\" @click=\"prevImage\">\n        <i class=\"bi bi-arrow-left-circle-fill\"></i>\n      </button>\n      <button class=\"carousel-button next\" @click=\"nextImage\">\n        <i class=\"bi bi-arrow-right-circle-fill\"></i>\n      </button>\n    </div>\n\n    <!-- Categories -->\n    <div class=\"category-section\">\n      <h2>Categories</h2>\n      <div v-if=\"categories.length\" class=\"category-list\">\n        <div\n          v-for=\"category in categories\"\n          :key=\"category.id\"\n          class=\"category-item\"\n        >\n          <h3>{{ category.name }}</h3>\n          <div v-if=\"category.products.length\" class=\"product-list-wrapper\">\n            <button\n              class=\"carousel-button prev\"\n              @click=\"prevProduct(category.id)\"\n            >\n              <i class=\"bi bi-arrow-left-circle-fill\"></i>\n            </button>\n            <div class=\"product-list-container\">\n              <div\n                class=\"product-list\"\n                :style=\"{\n                  transform: `translateX(-${\n                    getProductIndex(category.id) * 250\n                  }px)`,\n                }\"\n              >\n                <div\n                  v-for=\"product in category.products\"\n                  :key=\"product.id\"\n                  class=\"product-item\"\n                >\n                  <p>\n                    <strong>Name: {{ product.name }}</strong>\n                  </p>\n                  <p><strong>Price:</strong> {{ product.price }}</p>\n                  <button\n                    @click=\"requireLogin\"\n                    class=\"btn-add-cart\"\n                    :disabled=\"!product.stock\"\n                  >\n                    Add to Cart\n                  </button>\n                  <button\n                    @click=\"requireLogin\"\n                    class=\"btn-buy\"\n                    :disabled=\"!product.stock\"\n                  >\n                    Buy Now\n                  </button>\n                </div>\n              </div>\n            </div>\n            <button\n              class=\"carousel-button next\"\n              @click=\"nextProduct(category.id)\"\n            >\n              <i class=\"bi bi-arrow-right-circle-fill\"></i>\n            </button>\n          </div>\n\n          <p v-else>No products available in this category.</p>\n        </div>\n      </div>\n      <p v-else>No categories available.</p>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"WelCome\",\n  data() {\n    return {\n      categories: [],\n      showLoginOptions: false,\n      selectedRole: null,\n      carouselIndex: 0,\n      productIndices: {},\n      images: [\n        \"biscuits.jpeg\",\n        \"sliderr2.png\",\n        \"grain lowest.jpeg\",\n        \"vegetable -2.jpg\",\n      ],\n    };\n  },\n  methods: {\n    getProductIndex(categoryId) {\n      return this.productIndices[categoryId] || 0;\n    },\n    nextProduct(categoryId) {\n      const category = this.categories.find((cat) => cat.id === categoryId);\n      if (!category) return;\n\n      const maxIndex = Math.ceil(category.products.length - 1);\n      this.productIndices[categoryId] = Math.min(\n        this.getProductIndex(categoryId) + 1,\n        maxIndex\n      );\n    },\n    prevProduct(categoryId) {\n      this.productIndices[categoryId] = Math.max(\n        this.getProductIndex(categoryId) - 1,\n        0\n      );\n    },\n    nextImage() {\n      this.carouselIndex = (this.carouselIndex + 1) % this.images.length;\n    },\n    prevImage() {\n      this.carouselIndex =\n        (this.carouselIndex - 1 + this.images.length) % this.images.length;\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* General Styles */\nbody {\n  margin: 0;\n  padding: 0;\n  overflow-x: hidden; /* Prevent scrollbars */\n}\n\n/* Navbar */\n.navbar {\n  display: flex;\n  justify-content: space-between;\n  padding: 10px 20px;\n  background-color: #28a745;\n  color: white;\n}\n\n/* Carousel */\n.carousel-container {\n  position: relative;\n  overflow: hidden;\n}\n\n.carousel {\n  display: flex;\n  transition: transform 0.5s ease-in-out;\n}\n\n.carousel-image {\n  width: 100%;\n  flex: 0 0 auto; /* Prevent shrinking */\n}\n\n/* Category Section */\n.product-list-container {\n  overflow: hidden; /* Prevent scrollbars */\n}\n\n.product-item {\n  flex: 0 0 auto; /* Prevent shrinking */\n  width: 250px;\n}\n</style>\n"],"mappings":"AAqGA,eAAe;EACbA,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,gBAAgB,EAAE,KAAK;MACvBC,YAAY,EAAE,IAAI;MAClBC,aAAa,EAAE,CAAC;MAChBC,cAAc,EAAE,CAAC,CAAC;MAClBC,MAAM,EAAE,CACN,eAAe,EACf,cAAc,EACd,mBAAmB,EACnB,kBAAkB;IAEtB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,eAAeA,CAACC,UAAU,EAAE;MAC1B,OAAO,IAAI,CAACJ,cAAc,CAACI,UAAU,KAAK,CAAC;IAC7C,CAAC;IACDC,WAAWA,CAACD,UAAU,EAAE;MACtB,MAAME,QAAO,GAAI,IAAI,CAACV,UAAU,CAACW,IAAI,CAAEC,GAAG,IAAKA,GAAG,CAACC,EAAC,KAAML,UAAU,CAAC;MACrE,IAAI,CAACE,QAAQ,EAAE;MAEf,MAAMI,QAAO,GAAIC,IAAI,CAACC,IAAI,CAACN,QAAQ,CAACO,QAAQ,CAACC,MAAK,GAAI,CAAC,CAAC;MACxD,IAAI,CAACd,cAAc,CAACI,UAAU,IAAIO,IAAI,CAACI,GAAG,CACxC,IAAI,CAACZ,eAAe,CAACC,UAAU,IAAI,CAAC,EACpCM,QACF,CAAC;IACH,CAAC;IACDM,WAAWA,CAACZ,UAAU,EAAE;MACtB,IAAI,CAACJ,cAAc,CAACI,UAAU,IAAIO,IAAI,CAACM,GAAG,CACxC,IAAI,CAACd,eAAe,CAACC,UAAU,IAAI,CAAC,EACpC,CACF,CAAC;IACH,CAAC;IACDc,SAASA,CAAA,EAAG;MACV,IAAI,CAACnB,aAAY,GAAI,CAAC,IAAI,CAACA,aAAY,GAAI,CAAC,IAAI,IAAI,CAACE,MAAM,CAACa,MAAM;IACpE,CAAC;IACDK,SAASA,CAAA,EAAG;MACV,IAAI,CAACpB,aAAY,GACf,CAAC,IAAI,CAACA,aAAY,GAAI,IAAI,IAAI,CAACE,MAAM,CAACa,MAAM,IAAI,IAAI,CAACb,MAAM,CAACa,MAAM;IACtE;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDepe