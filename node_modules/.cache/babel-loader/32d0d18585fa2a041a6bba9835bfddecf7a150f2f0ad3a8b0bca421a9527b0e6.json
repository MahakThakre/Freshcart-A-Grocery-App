\" :key=\"category.id\" class=\"category-item\">\n    <h3>{{ category.name }}</h3>\n    <div v-if=\"category.products.length\" class=\"product-list-wrapper\">\n      <button\n        class=\"carousel-button prev\"\n        @click=\"prevProduct(category.id)\"\n        :disabled=\"getProductIndex(category.id) === 0\"\n      >\n        <i class=\"bi bi-arrow-left-circle-fill\"></i>\n      </button>\n      <div\n        class=\"product-list\"\n        :style=\"{\n          transform: `translateX(-${getProductIndex(category.id) * 250}px)`,\n        }\"\n      >\n        <div\n          v-for=\"product in category.products\"\n          :key=\"product.id\"\n          class=\"product-item\"\n        >\n          <p>\n            <strong>Name: {{ product.name }}</strong>\n          </p>\n          <p><strong>Price:</strong> {{ product.price }}</p>\n          <button\n            @click=\"requireLogin\"\n            class=\"btn-add-cart\"\n            :disabled=\"!product.stock\"\n          >\n            Add to Cart\n          </button>\n          <button\n            @click=\"requireLogin\"\n            class=\"btn-buy\"\n            :disabled=\"!product.stock\"\n          >\n            Buy Now\n          </button>\n        </div>\n      </div>\n      <button\n        class=\"carousel-button next\"\n        @click=\"nextProduct(category.id)\"\n        :disabled=\"\n          getProductIndex(category.id) === category.products.length - 1\n        \"\n      >\n        <i class=\"bi bi-arrow-right-circle-fill\"></i>\n      </button>\n    </div>\n    <p v-else>No products available in this category.</p>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"WelCome\",\n  data() {\n    return {\n      categories: [],\n      showLoginOptions: false,\n      selectedRole: null,\n      carouselIndex: 0,\n      productIndices: {},\n      images: [\n        \"biscuits.jpeg\",\n        \"sliderr2.png\",\n        \"grain lowest.jpeg\",\n        \"vegetable -2.jpg\",\n      ],\n    };\n  },\n  async mounted() {\n    try {\n      const response = await fetch(\"http://localhost:5000/api/welcomePage\");\n      if (!response.ok) {\n        throw new Error(`HTTP error! status: ${response.status}`);\n      }\n      this.categories = await response.json();\n    } catch (error) {\n      console.error(\"Error fetching welcome page data:\", error);\n    }\n\n    this.carouselInterval = setInterval(this.nextImage, 3000);\n  },\n  beforeUnmount() {\n    clearInterval(this.carouselInterval);\n  },\n  methods: {\n    getProductIndex(categoryId) {\n      return this.productIndices[categoryId] || 0;\n    },\n    nextProduct(categoryId) {\n      const category = this.categories.find((cat) => cat.id === categoryId);\n      if (!category) return;\n\n      const maxIndex = Math.max(0, category.products.length - 1);\n      const currentIndex = this.getProductIndex(categoryId);\n      if (currentIndex < maxIndex) {\n        this.productIndices[categoryId] = currentIndex + 1;\n      }\n    },\n    prevProduct(categoryId) {\n      const currentIndex = this.getProductIndex(categoryId);\n      if (currentIndex > 0) {\n        this.productIndices[categoryId] = currentIndex - 1;\n      }\n    },\n\n    selectRole(role) {\n      this.selectedRole = role;\n    },\n    navigateTo(path) {\n      this.showLoginOptions = false;\n      this.selectedRole = null;\n      this.$router.push(`/${path}`);\n    },\n    resetSelection() {\n      this.selectedRole = null;\n    },\n    capitalize(word) {\n      return word.charAt(0).toUpperCase() + word.slice(1);\n    },\n    nextImage() {\n      this.carouselIndex = (this.carouselIndex + 1) % this.images.length;\n    },\n    prevImage() {\n      this.carouselIndex =\n        (this.carouselIndex - 1 + this.images.length) % this.images.length;\n    },\n    requireLogin() {\n      this.showLoginOptions = true;\n    },\n    closeModal() {\n      this.showLoginOptions = false;\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* General Styles */\nbody {\n  font-family: Arial, sans-serif;\n  margin: 0;\n  padding: 0;\n}\n\n/* Navbar Styles */\n.navbar {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  background-color: #28a745;\n  color: white;\n  padding: 10px 20px;\n}\n\n.app-title {\n  margin: 0;\n  font-size: 24px;\n}\n\n.login-button {\n  background-color: white;\n  color: #28a745;\n  border: none;\n  padding: 10px 15px;\n  border-radius: 5px;\n  cursor: pointer;\n  transition: background-color 0.3s ease;\n}\n\n.login-button:hover {\n  background-color: #f0f0f0;\n}\n\n/* Welcome Section */\n.welcome-container {\n  text-align: center;\n  margin-top: 50px;\n}\n\n.welcome-title {\n  font-size: 36px;\n  color: #333;\n}\n\n/* Carousel Styles */\n.carousel-container {\n  position: relative;\n  width: 80%;\n  margin: 20px auto;\n  overflow: hidden;\n}\n\n.carousel {\n  display: flex;\n  transition: transform 0.5s ease-in-out;\n}\n\n.carousel-image {\n  width: 100%;\n  height: 350px;\n  flex-shrink: 0;\n}\n\n.carousel-button {\n  position: absolute;\n  top: 50%;\n  background: none;\n  border: none;\n  cursor: pointer;\n  font-size: 2rem;\n  color: white;\n  transform: translateY(-50%);\n}\n\n.carousel-button.prev {\n  left: 10px;\n}\n\n.carousel-button.next {\n  right: 10px;\n}\n\n.arrow {\n  font-size: 2rem;\n  color: #28a745;\n}\n\n/* Login Modal */\n.login-modal {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-color: rgba(0, 0, 0, 0.5);\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.modal-content {\n  background: white;\n  padding: 20px;\n  border-radius: 8px;\n  text-align: center;\n  width: 300px;\n}\n\n.product-list-wrapper {\n  position: relative;\n  display: flex;\n  align-items: center;\n  overflow: hidden; /* Ensures content doesn't overflow */\n}\n\n.product-list {\n  display: flex;\n  transition: transform 0.5s ease-in-out;\n  overflow: hidden;\n  width: 100%;\n}\n.carousel-button {\n  position: absolute;\n  top: 50%;\n  transform: translateY(-50%);\n  background: none;\n  border: none;\n  cursor: pointer;\n  font-size: 2rem;\n  color: #333;\n  z-index: 2; /* Ensure they are above products */\n}\n\n.carousel-button.prev {\n  left: 10px; /* Adjust to align with box */\n}\n\n.carousel-button.next {\n  right: 10px; /* Adjust to align with box */\n}\n\n.role-button {\n  display: block;\n  margin: 10px auto;\n  padding: 10px;\n  background-color: #0a0f14;\n  color: rgb(100, 26, 26);\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  width: 80%;\n}\n\n.role-button:hover {\n  background-color: #0056b3;\n}\n\n.close-button {\n  background-color: #dc3545;\n  color: white;\n  border: none;\n  padding: 10px;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.close-button:hover {\n  background-color: #c82333;\n}\n/* General styling */\n.category-section {\n  padding: 20px;\n  background-color: #f9f9f9a1;\n  border-radius: 10px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n  margin: 20px auto;\n  max-width: 1200px;\n}\n\n.category-section h2 {\n  font-size: 1.8rem;\n  color: #333;\n  margin-bottom: 15px;\n  text-align: center;\n}\n\n/* Category styling */\n.category-list {\n  display: block;\n  margin-bottom: 30px;\n}\n\n.category-item {\n  background: linear-gradient(to right, #a9f160, #87cefa);\n  padding: 15px;\n  border-radius: 8px;\n  border: 1px solid #ddd;\n  margin-bottom: 15px;\n}\n\n.category-item h3 {\n  font-size: 1.5rem;\n  color: #2b7a78;\n  margin-bottom: 10px;\n  text-align: center;\n}\n\n.product-item {\n  background: #fff;\n  border: 1px solid #ddd;\n  border-radius: 8px;\n  padding: 15px;\n  width: 220px; /* Adjusted width for balance */\n  height: 170px; /* Added fixed height */\n  text-align: center;\n  transition: box-shadow 0.3s ease;\n}\n\n.product-item:hover {\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n}\n\n.product-item p {\n  font-size: 0.9rem;\n  margin: 10px 0;\n}\n\n.product-item strong {\n  font-weight: 600;\n  color: #555;\n}\n\n/* Buttons */\n.btn-add-cart {\n  background-color: #4caf50; /* Green */\n  color: white;\n  padding: 8px 12px;\n  font-size: 0.85rem;\n  border-radius: 5px;\n  border: none;\n  cursor: pointer;\n  margin-top: 10px;\n  margin-right: 10px;\n  transition: background-color 0.3s ease;\n}\n\n.btn-add-cart:hover {\n  background-color: #45a049;\n}\n\n.btn-buy {\n  background-color: #ffeb3b; /* Yellow */\n  color: #555;\n  padding: 8px 12px;\n  font-size: 0.85rem;\n  border-radius: 5px;\n  border: none;\n  cursor: pointer;\n  margin-top: 10px;\n  transition: background-color 0.3s ease;\n}\n\n.btn-buy:hover {\n  background-color: #fdd835;\n}\n\n.btn-out-of-stock {\n  background-color: #f44336; /* Red */\n  color: white;\n  padding: 8px 12px;\n  font-size: 0.85rem;\n  border-radius: 5px;\n  border: none;\n  margin-top: 10px;\n  cursor: not-allowed;\n}\n\n.btn-add-cart:disabled,\n.btn-buy:disabled {\n  background-color: #ddd;\n  color: #aaa;\n  cursor: not-allowed;\n}\n\n/* Empty state for categories and products */\n.category-section p {\n  font-size: 1rem;\n  color: #888;\n  text-align: center;\n}\n\n.product-item p.no-stock {\n  color: #f44336;\n  font-weight: bold;\n  font-size: 1rem;\n}\n</style>\n"],"mappings":";;;EAGyCA,KAAK,EAAC;;;;;;;;;;2BAF7CC,mBAAA,CAoDMC,SAAA,QAAAC,WAAA,CApDkBC,KAAA,CAAAC,UAAU,EAAtBC,QAAQ;yBAApBL,mBAAA,CAoDM;MApD+BM,GAAG,EAAED,QAAQ,CAACE,EAAE;MAAER,KAAK,EAAC;QAC3DS,mBAAA,CAA4B,YAAAC,gBAAA,CAArBJ,QAAQ,CAACK,IAAI,kBACTL,QAAQ,CAACM,QAAQ,CAACC,MAAM,I,cAAnCZ,mBAAA,CAgDM,OAhDNa,UAgDM,GA/CJL,mBAAA,CAMS;MALPT,KAAK,EAAC,sBAAsB;MAC3Be,OAAK,EAAAC,MAAA,IAAEC,QAAA,CAAAC,WAAW,CAACZ,QAAQ,CAACE,EAAE;MAC9BW,QAAQ,EAAEF,QAAA,CAAAG,eAAe,CAACd,QAAQ,CAACE,EAAE;uCAEtCC,mBAAA,CAA4C;MAAzCT,KAAK,EAAC;IAA8B,2B,iCAEzCS,mBAAA,CA8BM;MA7BJT,KAAK,EAAC,cAAc;MACnBqB,KAAK,EAAAC,eAAA;kCAAwCL,QAAA,CAAAG,eAAe,CAACd,QAAQ,CAACE,EAAE;;2BAIzEP,mBAAA,CAuBMC,SAAA,QAAAC,WAAA,CAtBcG,QAAQ,CAACM,QAAQ,EAA5BW,OAAO;2BADhBtB,mBAAA,CAuBM;QArBHM,GAAG,EAAEgB,OAAO,CAACf,EAAE;QAChBR,KAAK,EAAC;UAEN