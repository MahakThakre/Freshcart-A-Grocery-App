{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"WelCome\",\n  data() {\n    return {\n      showLoginOptions: false,\n      selectedRole: null,\n      carouselIndex: 0,\n      images: [\"all grocery.png\", \"biscuits.jpeg\", \"fruits banner.jpg\", \"grain lowest.jpeg\"]\n    };\n  },\n  methods: {\n    selectRole(role) {\n      this.selectedRole = role;\n    },\n    navigateTo(path) {\n      this.showLoginOptions = false;\n      this.selectedRole = null;\n      this.$router.push(`/${path}`);\n    },\n    resetSelection() {\n      this.selectedRole = null;\n    },\n    capitalize(word) {\n      return word.charAt(0).toUpperCase() + word.slice(1);\n    },\n    nextImage() {\n      this.carouselIndex = (this.carouselIndex + 1) % this.images.length;\n    },\n    prevImage() {\n      this.carouselIndex = (this.carouselIndex - 1 + this.images.length) % this.images.length;\n    }\n  }\n};","map":{"version":3,"names":["name","data","showLoginOptions","selectedRole","carouselIndex","images","methods","selectRole","role","navigateTo","path","$router","push","resetSelection","capitalize","word","charAt","toUpperCase","slice","nextImage","length","prevImage"],"sources":["C:\\Users\\mahak\\Downloads\\21f2000845-freshcart\\Mad 2 project\\freshcart\\src\\components\\welcome.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <!-- Navbar -->\n    <nav class=\"navbar\">\n      <h1 class=\"app-title\">FreshCart App</h1>\n      <button class=\"login-button\" @click=\"showLoginOptions = true\">\n        Login / Sign Up\n      </button>\n    </nav>\n\n    <!-- Main Heading -->\n    <div class=\"welcome-container\">\n      <h2 class=\"welcome-title\">Welcome to FreshCart</h2>\n    </div>\n\n    <!-- Carousel Section -->\n    <div class=\"carousel-container\">\n      <div\n        class=\"carousel\"\n        :style=\"{ transform: 'translateX(' + -carouselIndex * 100 + '%)' }\"\n      >\n        <img\n          v-for=\"(image, index) in images\"\n          :key=\"index\"\n          :src=\"require(`@/photo/${image}`)\"\n          alt=\"carousel image\"\n          class=\"carousel-image\"\n        />\n      </div>\n      <button class=\"carousel-button prev\" @click=\"prevImage\">\n        <span class=\"arrow\">←</span>\n      </button>\n      <button class=\"carousel-button next\" @click=\"nextImage\">\n        <span class=\"arrow\">→</span>\n      </button>\n    </div>\n\n    <!-- Login Modal -->\n    <div v-if=\"showLoginOptions\" class=\"login-modal\">\n      <div class=\"modal-content\" v-if=\"!selectedRole\">\n        <h3>Select Your Role</h3>\n        <button class=\"role-button\" @click=\"selectRole('admin')\">Admin</button>\n        <button class=\"role-button\" @click=\"selectRole('manager')\">\n          Manager\n        </button>\n        <button class=\"role-button\" @click=\"selectRole('user')\">User</button>\n        <button class=\"close-button\" @click=\"showLoginOptions = false\">\n          Close\n        </button>\n      </div>\n\n      <div class=\"modal-content\" v-else>\n        <h3>Role: {{ capitalize(selectedRole) }}</h3>\n        <p>What would you like to do?</p>\n        <button\n          class=\"role-button\"\n          @click=\"navigateTo(`${selectedRole}_login`)\"\n        >\n          Login\n        </button>\n        <button\n          v-if=\"selectedRole !== 'admin'\"\n          class=\"role-button\"\n          @click=\"navigateTo(`${selectedRole}_signup`)\"\n        >\n          Sign Up\n        </button>\n        <button class=\"close-button\" @click=\"resetSelection\">Back</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"WelCome\",\n  data() {\n    return {\n      showLoginOptions: false,\n      selectedRole: null,\n      carouselIndex: 0,\n      images: [\n        \"all grocery.png\",\n        \"biscuits.jpeg\",\n        \"fruits banner.jpg\",\n        \"grain lowest.jpeg\",\n      ],\n    };\n  },\n  methods: {\n    selectRole(role) {\n      this.selectedRole = role;\n    },\n    navigateTo(path) {\n      this.showLoginOptions = false;\n      this.selectedRole = null;\n      this.$router.push(`/${path}`);\n    },\n    resetSelection() {\n      this.selectedRole = null;\n    },\n    capitalize(word) {\n      return word.charAt(0).toUpperCase() + word.slice(1);\n    },\n    nextImage() {\n      this.carouselIndex = (this.carouselIndex + 1) % this.images.length;\n    },\n    prevImage() {\n      this.carouselIndex =\n        (this.carouselIndex - 1 + this.images.length) % this.images.length;\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* General Styles */\nbody {\n  font-family: Arial, sans-serif;\n  margin: 0;\n  padding: 0;\n}\n\n/* Navbar Styles */\n.navbar {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  background-color: #28a745;\n  color: white;\n  padding: 10px 20px;\n}\n\n.app-title {\n  margin: 0;\n  font-size: 24px;\n}\n\n.login-button {\n  background-color: white;\n  color: #28a745;\n  border: none;\n  padding: 10px 15px;\n  border-radius: 5px;\n  cursor: pointer;\n  transition: background-color 0.3s ease;\n}\n\n.login-button:hover {\n  background-color: #f0f0f0;\n}\n\n/* Welcome Section */\n.welcome-container {\n  text-align: center;\n  margin-top: 50px;\n}\n\n.welcome-title {\n  font-size: 36px;\n  color: #333;\n}\n\n/* Carousel Styles */\n.carousel-container {\n  position: relative;\n  width: 80%;\n  margin: 20px auto;\n  overflow: hidden;\n}\n\n.carousel {\n  display: flex;\n  transition: transform 0.5s ease-in-out;\n}\n\n.carousel-image {\n  width: 100%;\n  height: 300px;\n  flex-shrink: 0;\n}\n\n.carousel-button {\n  position: absolute;\n  top: 50%;\n  background: none;\n  border: none;\n  cursor: pointer;\n  font-size: 2rem;\n  color: white;\n  transform: translateY(-50%);\n}\n\n.carousel-button.prev {\n  left: 10px;\n}\n\n.carousel-button.next {\n  right: 10px;\n}\n\n.arrow {\n  font-size: 2rem;\n  color: #28a745;\n}\n\n/* Login Modal */\n.login-modal {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-color: rgba(0, 0, 0, 0.5);\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.modal-content {\n  background: white;\n  padding: 20px;\n  border-radius: 8px;\n  text-align: center;\n  width: 300px;\n}\n\n.role-button {\n  display: block;\n  margin: 10px auto;\n  padding: 10px;\n  background-color: #007bff;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  width: 80%;\n}\n\n.role-button:hover {\n  background-color: #0056b3;\n}\n\n.close-button {\n  background-color: #dc3545;\n  color: white;\n  border: none;\n  padding: 10px;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.close-button:hover {\n  background-color: #c82333;\n}\n</style>\n"],"mappings":";AA0EA,eAAe;EACbA,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,gBAAgB,EAAE,KAAK;MACvBC,YAAY,EAAE,IAAI;MAClBC,aAAa,EAAE,CAAC;MAChBC,MAAM,EAAE,CACN,iBAAiB,EACjB,eAAe,EACf,mBAAmB,EACnB,mBAAmB;IAEvB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAACC,IAAI,EAAE;MACf,IAAI,CAACL,YAAW,GAAIK,IAAI;IAC1B,CAAC;IACDC,UAAUA,CAACC,IAAI,EAAE;MACf,IAAI,CAACR,gBAAe,GAAI,KAAK;MAC7B,IAAI,CAACC,YAAW,GAAI,IAAI;MACxB,IAAI,CAACQ,OAAO,CAACC,IAAI,CAAE,IAAGF,IAAK,EAAC,CAAC;IAC/B,CAAC;IACDG,cAAcA,CAAA,EAAG;MACf,IAAI,CAACV,YAAW,GAAI,IAAI;IAC1B,CAAC;IACDW,UAAUA,CAACC,IAAI,EAAE;MACf,OAAOA,IAAI,CAACC,MAAM,CAAC,CAAC,CAAC,CAACC,WAAW,CAAC,IAAIF,IAAI,CAACG,KAAK,CAAC,CAAC,CAAC;IACrD,CAAC;IACDC,SAASA,CAAA,EAAG;MACV,IAAI,CAACf,aAAY,GAAI,CAAC,IAAI,CAACA,aAAY,GAAI,CAAC,IAAI,IAAI,CAACC,MAAM,CAACe,MAAM;IACpE,CAAC;IACDC,SAASA,CAAA,EAAG;MACV,IAAI,CAACjB,aAAY,GACf,CAAC,IAAI,CAACA,aAAY,GAAI,IAAI,IAAI,CAACC,MAAM,CAACe,MAAM,IAAI,IAAI,CAACf,MAAM,CAACe,MAAM;IACtE;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}