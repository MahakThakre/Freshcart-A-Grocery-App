reshcart\\Mad 2 project\\freshcart\\src\\components\\display_product.vue"],"sourcesContent":["<template>\n  <div class=\"product-container\">\n    <h2>Products</h2>\n    <div v-if=\"products.length\" class=\"product-list\">\n      <div v-for=\"product in products\" :key=\"product.id\" class=\"product-card\">\n        <h3>{{ product.name }}</h3>\n        <p>Price: ₹{{ product.price }}</p>\n        <button @click=\"addToCart(product)\">Add to Cart</button>\n        <button @click=\"buyNow(product)\">Buy Now</button>\n      </div>\n    </div>\n    <p v-else>No products found in this category.</p>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"DisplayProduct\",\n  data() {\n    return {\n      products: [],\n    };\n  },\n  async mounted() {\n    try {\n      const categoryId = this.$route.params.id;\n      const response = await fetch(`http://localhost:5000/api/welcomePage`);\n      if (!response.ok) throw new Error(\"Failed to fetch\");\n\n      const categories = await response.json();\n      const category = categories.find((cat) => cat.id == categoryId); // Extract the matching category\n      this.products = category ? category.products : [];\n    } catch (error) {\n      console.error(\"Error fetching products:\", error);\n    }\n  },\n  methods: {\n    addToCart(product) {\n      console.log(\"Added to cart:\", product);\n    },\n    buyNow(product) {\n      console.log(\"Buying now:\", product);\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Linear Gradient Background */\n.product-container {\n  text-align: center;\n  padding: 20px;\n  background: whitesmoke;\n  min-height: 100vh;\n}\n\n/* Product Cards */\n.product-list {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: center;\n  gap: 20px;\n}\n\n.product-card {\n  background: linear-gradient(to right, #a9f160, #87cefa);\n  padding: 20px;\n  border-radius: 12px;\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);\n  transition: transform 0.3s ease-in-out;\n  width: 250px;\n}\n\n.product-card:hover {\n  transform: translateY(-5px);\n}\n\n.product-card h3 {\n  margin: 0;\n  font-size: 20px;\n  color: #333;\n}\n\n.product-card p {\n  font-size: 16px;\n  color: #555;\n}\n\n/* Buttons */\nbutton {\n  display: block;\n  width: 100%;\n  margin-top: 10px;\n  padding: 8px;\n  border: none;\n  border-radius: 6px;\n  font-size: 14px;\n  cursor: pointer;\n  transition: background 0.3s;\n}\n\nbutton:first-of-type {\n  background: #4caf50;\n  color: white;\n}\n\nbutton:first-of-type:hover {\n  background: #45a049;\n}\n\nbutton:last-of-type {\n  background: #ff5722;\n  color: white;\n}\n\nbutton:last-of-type:hover {\n  background: #e64a19;\n}\n</style>\n"],"mappings":";;EACOA,KAAK,EAAC;AAAmB;;;EAEAA,KAAK,EAAC;;;;;;;;uBAFpCC,mBAAA,CAWM,OAXNC,UAWM,G,0BAVJC,mBAAA,CAAiB,YAAb,UAAQ,sBACDC,KAAA,CAAAC,QAAQ,CAACC,MAAM,I,cAA1BL,mBAAA,CAOM,OAPNM,UAOM,I,kBANJN,mBAAA,CAKMO,SAAA,QAAAC,WAAA,CALiBL,KAAA,CAAAC,QAAQ,EAAnBK,OAAO;yBAAnBT,mBAAA,CAKM;MAL4BU,GAAG,EAAED,OAAO,CAACE,EAAE;MAAEZ,KAAK,EAAC;QACvDG,mBAAA,CAA2B,YAAAU,gBAAA,CAApBH,OAAO,CAACI,IAAI,kBACnBX,mBAAA,CAAkC,WAA/B,UAAQ,GAAAU,gBAAA,CAAGH,OAAO,CAACK,KAAK,kBAC3BZ,mBAAA,CAAwD;MAA/Ca,OAAK,EAAAC,MAAA,IAAEC,QAAA,CAAAC,SAAS,CAACT,OAAO;OAAG,aAAW,iBAAAU,UAAA,GAC/CjB,mBAAA,CAAiD;MAAxCa,OAAK,EAAAC,MAAA,IAAEC,QAAA,CAAAG,MAAM,CAACX,OAAO;OAAG,SAAO,iBAAAY,UAAA,E;qDAG5CrB,mBAAA,CAAiD,KAAAsB,UAAA,EAAvC,qCAAmC,G"},"metadata":{},"sourceType":"module","externalDependencies":[]}